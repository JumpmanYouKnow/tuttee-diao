void merge (struct llist *dest, struct llist *src1, struct llist *src2) {
	struct llnode* cleft = src1->front;
	struct llnode* cright = src2->front;

	//先找第一个
	if（cleft -> item < cright -> item) {
		dest->front = cleft;
		cleft = cleft -> next;
	}
	else {
		dest->front = cright;
		cright = cright -> next;
	}

	struct llnode* cdes = dest->front;

	while (cleft && cright) {
		if (cleft -> item < cright -> item) {
			cdes->next = cleft;
			cdes = cdes->next;
			cleft = cleft -> next;
		}
		else {
			cdes->next = cright;
			cdes = cdes->next;
			cright = cright -> next;
		}
	}

	while (cleft) {
		cdes->next = cleft;
		cdes = cdes -> next;
		cleft = cleft -> next;
	}


	while (cright) {
		cdes->next = cright;
		cdes = cdes -> next;
		cright = cright -> next;
	}


}